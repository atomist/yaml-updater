{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            true,
            {
                "import-sources-order": "any"
            }
        ]
    },
    "rulesDirectory": []
}